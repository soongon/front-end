
// window.alert('안녕하세요');

// 데이터 타입..
// number, string, boolean (true | false), array (object), object, function

var myNumber = 3;

console.log(typeof(myNumber));

var myString = 'hello world';
console.log(typeof(myString));

var myBool = true;
var myBool2 = false;

var aVar;
console.log(typeof(aVar));
console.log(aVar);

var anotherVar = null;

// arrays

var myArr = [1, 2, 3, 4, 5, 6];

console.log(myArr[2]);

console.log(typeof myArr);

myArr.reverse();

console.log(myArr);

myArr.forEach(function (e) {
    console.log(e);
});

// ECMA5
for (var i in myArr) {
    console.log(myArr[i]);
}

// ECMA6
for (var item of myArr) {
    console.log(item);
}

// function
function add(a, b) {
    return a + b;
}

// recommanded
var sub = function (a, b) {
    return a - b;
};

var result = add(3, 4);
console.log(result);

var res2 = sub(5, 3);
console.log(res2);

